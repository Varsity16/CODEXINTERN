def insertion_sort(alist):
    for i in range(1, len(alist)):
        temp = alist[i]
        j = i - 1
        while j >= 0 and temp < alist[j]:
            alist[j + 1] = alist[j]
            j -= 1
        alist[j + 1] = temp

# Get input from user
alist = input("Enter the list of numbers: ").split()
alist = [int(x) for x in alist]

# Perform sorting
insertion_sort(alist)

# Display sorted list
print("Sorted List:", alist)
